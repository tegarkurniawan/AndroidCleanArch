<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tegar.cleanapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".persentation.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CleanApp">
        <activity
            android:name=".persentation.register.RegisterActivity"
            android:theme="@style/Theme.CleanApp.NoActionBar" />
        <activity android:name=".persentation.login.LoginActivity"
            android:theme="@style/Theme.CleanApp.NoActionBar"
            >

        </activity>
        <activity
            android:name=".persentation.main.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.CleanApp.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>